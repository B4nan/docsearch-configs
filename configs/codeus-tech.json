{
  "indexName": "codeus-tech",
  "startUrls": [
    "https://codeustech.github.io/docs/xcs"
  ],
  "stopUrls": [],
  "selectors": {
    "text": ".post article p, .post article li",
    "lvl0": {
      "selector": "//*[contains(@class,'navGroups')]//*[contains(@class,'navListItemActive')]/preceding::h3[1]",
      "type": "xpath",
      "global": true,
      "defaultValue": "Documentation"
    },
    "lvl1": ".post h2",
    "lvl2": ".post h3",
    "lvl3": ".post h4",
    "lvl4": ".post h5",
    "lvl5": ".post h6"
  },
  "selectorsExclude": [
    ".hash-link"
  ],
  "sitemapUrls": [
    "https://codeustech.github.io/docs/xcs/sitemap.xml"
  ],
  "sitemapAlternateLinks": true,
  "customSettings": {
    "attributesForFaceting": [
      "language",
      "version"
    ]
  }
}